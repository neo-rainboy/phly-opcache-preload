{
    "name": "phly/phly-opcache-preload",
    "description": "Create opcache preload rules for an application",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas"
    ],
    "support": {
        "docs": "https://docs.laminas.dev/laminas-opcache-preload/",
        "issues": "https://github.com/phly/phly-opcache-preload/issues",
        "source": "https://github.com/phly/phly-opcache-preload",
        "rss": "https://github.com/phly/phly-opcache-preload/releases.atom",
        "forum": "https://discourse.laminas.dev/"
    },
    "require": {
        "php": "~7.4.4",
        "ocramius/package-versions": "^1.8",
        "symfony/console": "^4.1 || ^5.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~2.0.0",
        "phpunit/phpunit": "^9.1.4"
    },
    "autoload": {
        "psr-4": {
            "Phly\\OpcachePreload\\": "src//"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhlyTest\\OpcachePreload\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/phly-opcache-preload"
    ],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
